{% extends "base.html.twig" %}
{% block body %}
	<div class="container">
		<div class="row py-4">
			<div class="col">
				<h1>Survey Templates</h1>
				<ul class="list-group">
					{% for template in templates %}
					<li class="list-group-item">
						{{template.id}}
						{{template.name}}
					</li>
					{% endfor %}
				</ul>
			</div>
		</div>
	</div>
{% endblock %}